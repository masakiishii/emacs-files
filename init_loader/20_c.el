;; c and c++ mode setting(add-hook 'c-mode-hook	  '(lambda ()	     (gtags-mode t)	     (c-set-style "linux")	     (setq tab-width 4)	     (setq indent-tabs-mode t)	     (setq c-basic-offset tab-width))	  )(add-hook 'c++-mode-hook	  '(lambda ()	     (gtags-mode t)	     (c-set-style "linux")	     (setq tab-width 4)	     (setq indent-tabs-mode t)	     (setq c-basic-offset tab-width))	  );; gtags(autoload 'gtags-mode "gtags" "" t);; csharp-mode;;(require 'my-csharp-mode);;(require 'dash);;(require 'json);;(require 'popup);;(require 'flycheck);;(add-hook 'after-init-hook #'global-flycheck-mode);; quickrun.el for csharp;(require 'quickrun)